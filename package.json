{
  "name": "linter-latex",
  "main": "./lib/linter-latex",
  "version": "0.0.0",
  "description": "Compile and lint LaTeX files",
  "keywords": [
    "latex",
    "compile",
    "linter",
    "lint",
    "pdflatex",
    "xelatex",
    "luatex",
    "context"
  ],
  "repository": "https://github.com/reujab/linter-latex",
  "license": "Unlicense",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "configSchema": {
    "compiler": {
      "description": "Tool used to compile LaTeX into a PDF",
      "type": "string",
      "default": "pdflatex",
      "enum": [
        {"value": "pdflatex", "description": "pdflatex"},
        {"value": "xelatex", "description": "xelatex"},
        {"value": "lualatex", "description": "lualatex"}
      ]
    },
    "extraArguments": {
      "description": "Extra arguments to pass to the compiler (separated by a comma)",
      "type": "array",
      "default": [],
      "items": {
        "type": "string"
      }
    }
  }
}
